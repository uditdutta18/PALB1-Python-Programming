class Solution:
    # Function to check if b is a subset of a
    def isSubset(self, a, b):
        freq = {}

        # Count frequency of elements in a
        for x in a:
            freq[x] = freq.get(x, 0) + 1

        # Check elements of b
        for x in b:
            if x not in freq or freq[x] == 0:
                return False
            freq[x] -= 1

        return True
